#In terminal 1 start
mosquitto_sub -h www.geospaces.org -p 8883 -v -d -t hello/world -u owntracks -P a

#In terminal 2
mosquitto_pub -h www.geospaces.org -p 8883 -d -t hello/world -u owntracks -P a -m "Sada"



mosquitto_pub -h www.geospaces.org -p 8883 -d -t hello/world  -u owntracks -q 2 -P a -m "Sada1" --id "c1"

